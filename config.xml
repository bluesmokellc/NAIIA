<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coconnect.naiia" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NAIIA</name>
    <description>
        App for the NAIIA 2014 Convention
    </description>
    <author email="jim@coconnect.co" href="http://www.naiia.com/">
        Coconnect
    </author>
    <icon src="logo.png" />
    <content src="index.html" />
    <access origin="*" />
